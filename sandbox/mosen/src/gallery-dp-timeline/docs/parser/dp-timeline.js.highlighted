<span class="cm">         */</span>
        <span class="nx">isDatePublicHoliday</span> <span class="o">:</span> <span class="kd">function</span><span class="p">(</span><span class="nx">d</span><span class="p">)</span> <span class="p">{</span>
            <span class="c1">//Y.log(&quot;isDatePublicHoliday&quot;, &quot;info&quot;, &quot;Y.DP.Timeline&quot;);</span>
            
