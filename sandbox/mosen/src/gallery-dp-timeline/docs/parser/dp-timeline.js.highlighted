            <span class="k">return</span> <span class="nx">offset</span><span class="p">;</span>
        <span class="p">},</span>
        
        <span class="cm">/**</span>
<span class="cm">         * Convert the supplied slot number into a y offset</span>
<span class="cm">         *</span>
<span class="cm">         * @method slotToOffset</span>
<span class="cm">         * @param slot {Number} Slot number</span>
<span class="cm">         * @private</span>
<span class="cm">         */</span>
