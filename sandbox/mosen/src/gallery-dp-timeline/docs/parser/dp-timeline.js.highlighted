        <span class="nx">getEndDate</span> <span class="o">:</span> <span class="kd">function</span><span class="p">()</span> <span class="p">{</span>
            <span class="nx">Y</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="s2">&quot;getEndDate&quot;</span><span class="p">,</span> <span class="s2">&quot;info&quot;</span><span class="p">,</span> <span class="s2">&quot;Y.DP.Timeline&quot;</span><span class="p">);</span>
            
            <span class="c1">// One day is deducted because the word length implies that the starting day is included in that range</span>
