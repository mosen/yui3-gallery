            <span class="c1">//Y.log(&quot;onBackgroundHover&quot;, &quot;info&quot;, &quot;Y.DP.Timeline&quot;);</span>
            <span class="c1">//console.dir(e);</span>
            
            <span class="kd">var</span> <span class="nx">xy</span> <span class="o">=</span> <span class="p">[</span> <span class="nx">e</span><span class="p">.</span><span class="nx">clientX</span><span class="p">,</span> <span class="nx">e</span><span class="p">.</span><span class="nx">clientY</span> <span class="p">];</span>
            <span class="c1">//this._nodeEventInsertHighlight.set(&#39;style.top&#39;, e.clientY + &#39;px&#39;);</span>
            <span class="c1">//this._nodeEventInsertHighlight.set(&#39;style.left&#39;, e.clientX + &#39;px&#39;);</span>
        <span class="p">},</span>
        
        <span class="c1">// Date math functions</span>
        
        <span class="cm">/**</span>
<span class="cm">         * Get the last date that should be shown on the timeline</span>
<span class="cm">         *</span>
